{
  "name": "react-component-store",
  "version": "1.0.0",
  "description": "State management library for React applications.",
  "author": {
    "name": "Jyoti Puri",
    "email": "jyotipuri@gmail.com",
    "url": "http://jpuri.github.io/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jpuri/react-component-stores.git"
  },
  "main": "lib/index.js",
  "keywords": [
    "react"
  ],
  "scripts": {
